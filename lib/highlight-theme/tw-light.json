  "displayName": "Thoughtworks Light",
  "name": "tw-light",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language"
      ],
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#634f7d"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#6b9e78"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f2617a"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#f2617a"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#003d4f"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#cc850a"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bd4257"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bd4257"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bd4257"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bd4257"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "background": "#f2617a",
        "content": "^M",
        "fontStyle": "italic underline",
        "foreground": "#edf1f3"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#bd4257"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#003d4f"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#003d4f"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6b9e78"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#cc850a"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6b9e78"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#f2e9e0",
        "foreground": "#bd4257"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#edf1f3",
        "foreground": "#6b9e78"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#f2e9e0",
        "foreground": "#cc850a"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "background": "#47a1ad",
        "foreground": "#edf1f3"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#634f7d"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#47a1ad"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#bd4257"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#003d4f"
      }
    }
  ],
  "type": "light"
}